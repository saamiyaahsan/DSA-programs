#include <stdio.h>

int main()
{   char A[] = "SaraS";
    char B[6];
    int i,j,flag=0;
    for(i=0;A[i]!='\0';i++)
    {
        
    }
    i=i-1;
    for(j=0;i>=0;i--,j++)
    {
        B[j] = A[i];
    }
   
    printf("The reverse of a string is %s",B);
    printf("\nThe original string is %s",A);
    i=0;
    for(i=0;A[i]!='\0';i++)
    {
        if(A[i]!=B[i])
        flag=1;
    }
    if(flag == 0)
    printf("\nGiven string is a palindrome");
    else
    printf("\nGiven string is not a palindrome");
    return 0;
}



